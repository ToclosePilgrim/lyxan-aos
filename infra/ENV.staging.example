# Local staging env (template)
# Usage:
# - copy to infra/ENV.staging (or export env vars manually)
# - run: docker compose -f infra/docker-compose.staging.yml --env-file infra/ENV.staging up --build

BACKEND_PORT=3001
FRONTEND_PORT=3000
VERSION=local-staging

# Used by backend container
BACKEND_DATABASE_URL=postgresql://aos:aos@postgres:5432/aosdb_staging?schema=public
REDIS_URL=redis://redis:6379
JWT_SECRET=changeme

ACCOUNTING_VALIDATE_ON_POST=true
LEGACY_STOCK_ENABLED=false
OS_SELF_VALIDATE_MODE=strict

NEXT_PUBLIC_API_URL=http://localhost:3001/api















